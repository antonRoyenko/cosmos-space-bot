    "@babel/plugin-transform-react-jsx": "7.18.6",
    "@babel/plugin-transform-runtime": "7.18.9",
    "@babel/preset-env": "7.18.9",
    "@babel/preset-react": "7.18.6",
    "@types/big.js": "6.1.5",
    "@types/debug": "4.1.7",
    "@types/fs-extra": "9.0.13",
    "@types/ioredis": "4.28.10",
