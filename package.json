    "big.js": "6.2.0",
    "copyfiles": "2.4.1",
    "cosmwasm": "1.1.1",
    "countries-and-timezones": "3.3.0",
    "csv-parse": "5.3.0",
    "dayjs": "1.11.3",
    "dotenv": "16.0.1",
    "envalid": "7.3.1",
    "eslint-plugin-react": "7.30.1",
    "fastify": "4.2.0",
    "fastify-cron": "1.3.1",
    "fs-extra": "10.1.0",
