{
  "name": "cosmos-space-bot",
  "version": "0.0.1",
  "description": "Cosmos space bot",
  "main": "dist/run.js",
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts,.tsx ./bot/src ./server",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "tsc-watch --onSuccess \"npm run start\"",
    "start": "node dist/server/run.js",
    "heroku-prebuild": "npm i --only=prod",
    "copyAssets": "copyfiles --error --up 1 server/assets/*.* dist/server",
    "postbuild": "npm run copyAssets"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fastify/cors": "8.0.0",
