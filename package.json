{
  "name": "cosmos-space-bot",
  "version": "0.0.1",
  "description": "Cosmos space bot",
  "main": "dist/run.js",
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "fix:eslint": "eslint --fix \"src/*.*\"",
    "fix:prettier": "prettier --write \"src/*.*\"",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "tsc-watch --onSuccess \"npm run start\"",
    "start": "node dist/run.js",
    "prepare": "husky install",
    "graphql:codegen": "graphql-codegen -c ./src/graphql/desmos/codegen.yml"
  },
  "author": "deptyped <deptyped@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@grammyjs/fluent": "1.0.3",
    "@grammyjs/menu": "1.1.2",
    "@grammyjs/parse-mode": "1.1.2",
    "@grammyjs/ratelimiter": "1.1.5",
    "@grammyjs/router": "2.0.0",
    "@grammyjs/storage-redis": "2.0.0",
    "@grammyjs/transformer-throttler": "1.1.2",
    "@grammyjs/types": "2.8.1",
    "@moebius/fluent": "1.1.0",
    "@prisma/client": "4.0.0",
    "bech32": "2.0.0",
    "big.js": "6.2.0",
    "dayjs": "1.11.3",
    "dotenv": "16.0.1",
    "envalid": "7.3.1",
    "fastify": "4.2.0",
    "grammy": "1.9.0",
    "grammy-guard": "0.3.1",
    "graphql": "16.5.0",
    "graphql-request": "4.3.0",
    "ioredis": "5.1.0",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "node-fetch": "2.6.7",
    "numeral": "2.0.6",
    "pino": "8.1.0",
    "pino-pretty": "8.1.0",
    "prom-client": "14.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.7.0",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/typescript": "2.6.0",
    "@graphql-codegen/typescript-operations": "2.4.3",
    "@types/big.js": "6.1.5",
    "@types/debug": "4.1.7",
    "@types/ioredis": "4.28.10",
    "@types/lodash": "4.14.182",
    "@types/node": "18.0.0",
    "@types/node-fetch": "2.6.2",
    "@types/numeral": "2.0.2",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "3.1.1",
    "eslint-module-utils": "2.7.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prisma": "4.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.8.1",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "_moduleAliases": {
    "@bot": "dist"
  }
}
